<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        
        <resource-bundle>
            <base-name>/propiedadesEtiquetaEC</base-name>
            <var>propiedadesEtiquetaBL</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>/propiedadesTituloEC</base-name>
            <var>propiedadesTituloBL</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>/propiedadesObjetosEC</base-name>
            <var>propiedadesObjetosBL</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>/propiedadesMensajesEC</base-name>
            <var>propiedadesMensajesBL</var>
        </resource-bundle>
        
      
    </application>    
    <lifecycle>
        <phase-listener>ec.renafipse.mks.capas.sesion.SesionPhaseListener</phase-listener>
    </lifecycle>
</faces-config>
