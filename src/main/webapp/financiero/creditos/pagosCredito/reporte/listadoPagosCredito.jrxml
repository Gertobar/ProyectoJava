	<queryString>
		<![CDATA[SELECT    tii.siglas||' '||ifi.nombre "IFIP",
          sol.numero                "CREDITO",
          per.nombre_completo       "SOCIO",
          mon.nombre                "MONEDA",
          sol.monto_credito         "CANTIDAD SOLICITADA"
          
FROM      mks_creditos.solicitud    sol,
          mks_socios.socio          soc,
          mks_socios.persona        per,
          mks_comunes.moneda        mon,
          mks_ifips.ifip            ifi,
          mks_ifips.tipo_ifip       tii
WHERE     sol.numero                = $P{numeroCredito}
AND       sol.codigo_ifip           = $P{codigoIfip}
AND       sol.codigo_socio          = soc.codigo          
AND       sol.codigo_ifip           = soc.codigo_ifip
AND       per.codigo                = soc.codigo_persona
AND       sol.codigo_moneda         = mon.codigo
AND       sol.codigo_ifip           = ifi.codigo
AND       ifi.codigo_tipo_ifip      = tii.codigo]]>
	</queryString>
