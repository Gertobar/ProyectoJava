	<subDataset name="detallePagare" uuid="f740caf8-fd36-4967-b101-32d5502dfb45">
		<parameter name="numCredito" class="java.lang.Long"/>
		<parameter name="codIfip" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT tc.nombre                        "TIPO CARTERA",
       tm.dia_inicia                    "DIA INICIA",
       tm.dia_fin                       "DIA FIN",
       tm.interes_mora                  "INTERES MORA",
       TO_CHAR(tm.fecha_inicial_vigencia,'dd/mm/yyyy')        "FECHA INICIAL VIGENCIA",
       TO_CHAR(tm.fecha_final_vigencia,'dd/mm/yyyy')          "FECHA FINAL VIGENCIA"
FROM   mks_creditos.solicitud s,
       mks_creditos.producto_credito pc,
       mks_creditos.producto_credito_moneda pcm,
       mks_creditos.producto_credito_moneda_ifip pcmi,
       mks_creditos.tipo_cartera tc,
       mks_creditos.tasa_mora tm
WHERE  s.numero=$P{numCredito}
AND    s.codigo_producto=pcmi.codigo_producto
AND    pc.codigo = pcm.codigo_producto
AND    pcm.codigo_producto=pcmi.codigo_producto
AND    pc.codigo_tipo_cartera=tc.codigo
AND    tm.codigo_tipo_cartera=tc.codigo]]>
		</queryString>
		<field name="TIPO CARTERA" class="java.lang.String"/>
		<field name="DIA INICIA" class="java.math.BigDecimal"/>
		<field name="DIA FIN" class="java.math.BigDecimal"/>
		<field name="INTERES MORA" class="java.math.BigDecimal"/>
		<field name="FECHA INICIAL VIGENCIA" class="java.lang.String"/>
		<field name="FECHA FINAL VIGENCIA" class="java.lang.String"/>
	</subDataset>
	<subDataset name="conyuge" uuid="6f0e1264-4943-4213-bdb1-216faeb7b65a">
		<queryString language="SQL">
			<![CDATA[SELECT per.identificacion,
       per.nombre_completo
FROM   mks_creditos.solicitud              sol,
       mks_socios.socio                    soc,
       mks_socios.persona_conyuge          pec,
       mks_socios.persona                  per
WHERE  sol.numero                          = 2
AND    sol.codigo_ifip                     = 2
AND    firma                               = 'S'
AND    eliminado                           = 'N'
AND    soc.codigo                          = sol.codigo_socio
AND    soc.codigo_ifip                     = sol.codigo_ifip
AND    pec.codigo_persona                  = soc.codigo_persona
AND    per.codigo                          = pec.codigo_persona_conyuge]]>
		</queryString>
		<field name="IDENTIFICACION" class="java.lang.String"/>
		<field name="NOMBRE_COMPLETO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="garantes" uuid="e6612abf-fdfa-40e1-9275-bf00051dc56b">
		<queryString>
			<![CDATA[WITH
   conyuge  AS (SELECT pey.identificacion, pey.nombre_completo,
                       pec.codigo_persona
                FROM   mks_creditos.garante_credito      gac,
                       mks_socios.persona                peg,
                       mks_socios.persona_conyuge        pec,
                       mks_socios.persona                pey
                WHERE  gac.numero_credito                = 2
                AND    gac.codigo_ifip                   = 2
                AND    gac.eliminado                     = 'N'
                AND    pec.firma                         = 'S'
                AND    pec.eliminado                     = 'N'
                AND    pec.codigo_persona                = gac.codigo_persona
                AND    peg.codigo                        = pec.codigo_persona
                AND    pey.codigo                       = pec.codigo_persona_conyuge )
SELECT (SELECT identificacion
        FROM   mks_socios.persona
        WHERE  codigo                   = gac.codigo_persona) "IDENTIFICACION",
        (SELECT nombre_completo
        FROM   mks_socios.persona
        WHERE  codigo                   = gac.codigo_persona) "NOMBRE",
        '......................'               "FIRMA1",
        (SELECT identificacion
         FROM conyuge
         WHERE codigo_persona           = gac.codigo_persona)  "INDENTIFACION CONYUGE",
         (SELECT nombre_completo
         FROM conyuge
         WHERE codigo_persona           = gac.codigo_persona)  "NOMBRE CONYUGE",
         CASE WHEN (SELECT identificacion
                     FROM conyuge
                     WHERE codigo_persona           = gac.codigo_persona) IS NOT NULL
         THEN '......................'   ELSE NULL END             "FIRMA C"
FROM   mks_creditos.garante_credito      gac
WHERE  numero_credito                    = 2
AND    codigo_ifip                       = 2
AND    eliminado                         = 'N']]>
		</queryString>
		<field name="IDENTIFICACION" class="java.lang.String"/>
		<field name="NOMBRE" class="java.lang.String"/>
		<field name="FIRMA1" class="java.lang.String"/>
		<field name="INDENTIFACION CONYUGE" class="java.lang.String"/>
		<field name="NOMBRE CONYUGE" class="java.lang.String"/>
		<field name="FIRMA C" class="java.lang.String"/>
	</subDataset>
