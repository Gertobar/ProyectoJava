	<parameter name="subReporteTablaAmortizacion" class="java.lang.Object"/>
	<queryString>
		<![CDATA[WITH
  conyuge AS (SELECT ppp.identificacion,
          ppp.nombre_completo,
          pec.codigo_persona
     FROM   mks_creditos.solicitud                      sol,
          mks_creditos.producto_credito_moneda_ifip   pcm,
          mks_socios.socio                            soc,
          mks_socios.persona_conyuge                  pec,
          mks_socios.persona                          ppp
     WHERE  sol.numero                                  = $P{numeroCredito}
     AND    sol.codigo_ifip                             = $P{codigoIfip}
             AND    pcm.firma_conyuge                           = 'S'
             AND    firma                                = 'S'
     AND    pec.eliminado                               = 'N'
     AND    pcm.firma_conyuge                           = 'S'
     AND    soc.codigo                                  = sol.codigo_socio
     AND    soc.codigo_ifip                             = sol.codigo_ifip
     AND    pec.codigo_persona                          = soc.codigo_persona
     AND    ppp.codigo                                  = pec.codigo_persona_conyuge
     AND    pcm.codigo_producto                         = sol.codigo_producto
     AND    pcm.codigo_moneda                           = sol.codigo_moneda
     AND    pcm.codigo_ifip                             = sol.codigo_ifip)
SELECT sp.numero_pagare                                               "PAGARE A LA ORDEN No:",
       soc.codigo                                                     "SOCIO No:",
       per.nombre_completo                                            "NOMBRE:",
       per.identificacion                                             "C.I.:",
       tii.siglas||' '||ifi.nombre                                    "IFIP",
       sol.numero                                                     "CREDITO No:",
       sol.monto_credito                                              "POR",
     (SELECT TO_CHAR(sysdate,'DD')FROM DUAL )                         "DIA",
     (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL)                     "MES",
     (SELECT TO_CHAR(sysdate,'YYYY') FROM DUAL)                       "ANIO",
      (SELECT MAX(TRUNC(tab.fecha_pago))
          FROM   mks_creditos.tabla_amortizacion tab
          WHERE  numero_credito         = sol.numero
          AND    codigo_ifip            = sol.codigo_ifip)           "VENCIMIENTO",
      REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 9)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$MONTO_LETRAS',(mks_creditos.pkm_numero_a_letras.DinTex(NVL(sol.monto_credito,0)))||' dolares'),'$TASA',TO_CHAR(sol.tasa,'990.00')),'$CARTERA',car.nombre),'$EFECT',round((power(sol.tasa/(12*100)+1,12)-1)*100,2)),'$CUOTAS',sol.numero_cuotas)     "PARRAFO1",

        REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 10)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                                                  "PARRAFO2",

        REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 11)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$LUGAR', ubc.nombre)                                                                                  "PARRAFO3",

        REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 12)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$LUGAR',ubc.nombre)
                ,'$DIA', (SELECT TO_CHAR(sysdate,'DD')FROM DUAL ))
                ,'$MES', (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL ))
                ,'$ANIO', (SELECT TO_CHAR(sysdate,'YYYY') FROM DUAL) )                                                   "PARRAFO4",

        REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 13)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                                                  "PARRAFO5",

        REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 14)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$LUGAR',ubc.nombre)
                ,'$DIA', (SELECT TO_CHAR(sysdate,'DD')FROM DUAL ))
                ,'$MES', (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL ))
                ,'$ANIO', (SELECT TO_CHAR(sysdate,'YYYY') FROM DUAL))                                                  "PARRAFO6",

        (SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 34)                                                  "PARRAFO7",
                conyuge.identificacion                                                                                 "IDENTIFICACION CONYUGE",
                conyuge.nombre_completo                                                                                "NOMMBRE CONYUGE",
                car.nombre                                                                                             "CARTERA",

         round((power(sol.tasa/(12*100)+1,12)-1)*100,2)                                                                 "EFECTIVA",
  pro.nombre                    "PRODUCTO",

         REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 4
                AND     codigo_fragmento                   = 35)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$LUGAR',ubc.nombre)
                ,'$DIA', (SELECT TO_CHAR(sysdate,'DD')FROM DUAL ))
                ,'$MES', (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL ))
                ,'$ANIO', (SELECT TO_CHAR(sysdate,'YYYY') FROM DUAL))                                                  "PARRAFO8",
                ifa.nombre                                                                                                 "AGENCIA",
                peu.Nombre_Completo                                                                                                "OFICIAL DE CREDITO",
                des.nombre                                                                                                         "DESTINO",
                sol.tasa                                                                                                           "TASA",
                sol.numero_cuotas                                                                                                  "CUOTAS",
               prd.nombre                                                                                                          "PERIODICIDAD"





FROM   mks_creditos.solicitud                                         sol,
       mks_socios.socio                                               soc,
       mks_socios.persona                                             per,
       mks_ifips.ifip_agencia                                         ifa,
       mks_ifips.ifip                                                 ifi,
       mks_ifips.tipo_ifip                                            tii,
       mks_creditos.solicitud_pagare                                  sp,
       mks_comunes.ubicacion_geografica                               ubc,
       mks_creditos.producto_credito                                  pro,
       mks_creditos.tipo_cartera                                      car,
        mks_socios.persona                                            peu,
        mks_seguridades.usuario                                        usu,
        mks_creditos.destino_financiero                                des,
        mks_comunes.periodicidad                                       prd,
       conyuge
WHERE  sol.numero                                                     = $P{numeroCredito}
AND    sol.codigo_ifip                                                = $P{codigoIfip}
AND    sol.codigo_socio                                               = soc.codigo
AND    sol.codigo_ifip                                                = ifi.codigo
AND    sol.codigo_ifip_agencia                                        = ifa.codigo
AND    ifi.codigo_tipo_ifip                                           = tii.codigo
AND    soc.codigo_persona                                             = per.codigo
AND    ubc.codigo                                                     = ifa.codigo_ubi_geo_ciudad
AND    sol.numero                                                     = sp.numero_credito
AND    sol.codigo_producto                                            = pro.codigo
AND    pro.codigo_tipo_cartera                                        = car.codigo
AND     sol.estado_colocado_por                                        = usu.codigo
AND     usu.codigo_persona                                             = peu.codigo
AND     sol.codigo_destino_financiero                                  =des.codigo
AND     sol.codigo_periodicidad                                        =prd.codigo
AND    soc.codigo_persona                                             = conyuge.codigo_persona (+)]]>
	</queryString>
