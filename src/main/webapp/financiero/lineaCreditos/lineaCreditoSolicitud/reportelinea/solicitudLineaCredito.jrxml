	<parameter name="codigoLineaCredito" class="java.lang.Long"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:/Renafipse/minkasoftware/src/main/webapp/financiero/creditos/solicitud/reporte1/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoIfip" class="java.lang.Long"/>
	<parameter name="subReporteTablaAmortizacion" class="java.lang.Object"/>
	<parameter name="subReporteInformacionSocio" class="java.lang.Object"/>
	<queryString>
		<![CDATA[WITH  
  conyuge AS (SELECT ppp.identificacion   identificacion,
                     ppp.nombre_completo  nombre,
                     pec.codigo_persona   codigo_persona,
    pec.codigo_persona_conyuge  cod_per_con,
                     (SELECT    act.nombre
                      FROM      mks_socios.actividad_economica   act,
                                mks_socios.persona_act_eco_pri   pap
                      WHERE     pap.codigo_actividad_economica   = act.codigo
                      AND       pap.codigo_persona               = ppp.codigo) actividad_economica,
                      (SELECT   pet.numero                       
                       FROM     mks_socios.persona_telefono      pet
                       WHERE    pet.codigo_persona               = ppp.codigo
                       AND      pet.eliminado                    = 'N'
                       AND      ROWNUM                           = 1)          telefono,
                      (SELECT   pes.direccion
                       FROM     mks_socios.persona_residencia    pes
                       WHERE    pes.codigo_persona               = ppp.codigo) direccion
              FROM   mks_creditos.solicitud              sol,
                     mks_socios.socio                    soc,
                     mks_socios.persona_conyuge          pec,
                     mks_socios.persona                  ppp
              WHERE  sol.numero                          = $P{codigoLineaCredito}
              AND    sol.codigo_ifip                     = $P{codigoIfip}
              AND    firma                               = 'S' 
              AND    eliminado                           = 'N'
              AND    soc.codigo                          = sol.codigo_socio
              AND    soc.codigo_ifip                     = sol.codigo_ifip
              AND    pec.codigo_persona                  = soc.codigo_persona
              AND    ppp.codigo                          = pec.codigo_persona_conyuge)       
SELECT  lcs.codigo                                                     "SOLICITUD N°",
        soc.codigo                                                     "SOCIO No:",
        per.nombre_completo                                            "NOMBRES COMPLETOS",
        tid.nombre                                                     "TIPO IDENTIFICACION",
        per.identificacion                                             "IDENTIFICACION",
        esc.nombre                                                     "ESTADO CIVIL",
        ins.nombre                                                     "INSTRUCCION",
        prf.nombre                                                     "PROFESION",
        tii.nombre||' '||ifi.nombre                                    "IFIP",
        lcs.codigo                                                     "CREDITO No:" , 
        tic.nombre                                                     "TIPO CARTERA",
        pro.nombre                                                     "PRODUCITO",
        mon.nombre                                                     "MONEDA",
        lcs.monto_linea_credito                                        "MONTO CREDITO",
        act.nombre                                                     "DESTINO CREDITO",
        NVL((SELECT de.nombre 
             FROM mks_creditos.destino_especifico de 
             WHERE lcs.codigo_destino_especifico = de.codigo), lcs.observaciones) "DESTINO ESPECIFICO",
        prd.nombre                                                     "PERIODICIDAD",
        lcs.observaciones                                              "OBSERVACIONES",
        pes.direccion                                                  "DIRECCION",
        pes.referencia                                                 "REFERENCIA",
        lc.dia_genera_credito                                          "DIA CORTE",
        (SELECT ti.valor
        FROM   mks_creditos.linea_credito lc1,
               mks_creditos.producto_credito_moneda_ifip pcmi,
               mks_creditos.tasa_interes_pro_cre_mon_ifi tipc,
               mks_creditos.tasa_interes_credito_ifip ti
        WHERE  lc1.codigo_producto = pcmi.codigo_producto
        AND    lc1.codigo_moneda = pcmi.codigo_moneda
        AND    lc1.codigo_ifip = pcmi.codigo_ifip
        AND    pcmi.codigo_producto = tipc.codigo_producto
        AND    pcmi.codigo_moneda = tipc.codigo_moneda
        AND    pcmi.codigo_ifip = tipc.codigo_ifip
        AND    tipc.codigo_tasa = ti.codigo
        AND    lc1.codigo = lc.codigo
        AND    tipc.eliminado = 'N') "TASA CREDITO",
        DECODE(lc.tipo_tabla,'P','FRANCESA','ALEMANA')                "TABLA DE AMORTIZACION",
        pen.cargas_familiares                                          "CARGAS FAMILIARES",
        pes.barrio                                                     "BARRIO",
        mks_socios.fn_obtiene_lugar_jerarquico(pes.codigo_ubi_geo_res) "PAIS/PRO/CAN/PAR",
        coy.identificacion                                            "IDENTIFICACION CONYUGE",
        coy.nombre                                                    "NOMBRE CONYUGE",
        coy.actividad_economica                                       "ACTIVIDAD ECONOMICA COY",
        coy.telefono                                                  "TELEFONO CONYUGE",
        coy.direccion                                                 "DIRECCION CONYUGE",
        REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 8
                AND     codigo_fragmento                   = 1)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                  "NOTA BIEN",
REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 8
                AND     codigo_fragmento                   = 9)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                  "PARRAFO DOS",                
        REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 8
                AND     codigo_fragmento                   = 11)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                  "PARRAFO TRES",                
                (SELECT p.nombre_completo
                  FROM   mks_auditorias.linea_credito_solicitud_estado e,
                         mks_seguridades.usuario u,
                         mks_socios.persona p
                  WHERE  e.codigo_usuario = u.codigo
                  AND    u.codigo_persona = p.codigo
                  AND    e.codigo_linea_credito_sol = $P{codigoLineaCredito}
                  AND    e.codigo_linea_cre_est_sol = $P{codigoIfip}) "OFICIAL DE CREDITO",
pro.codigo_tipo_cartera,
        ubg.nombre                                                     "CIUDAD",
        CASE lc.tipo_tabla
             WHEN 'C' THEN 'CAPITAL FIJO (METODO ALEMAN). VALOR PRIMERA CUOTA: '||(SELECT t.total FROM mks_creditos.tabla_amortizacion t WHERE t.numero_credito=lcs.codigo and t.cuota=1)
             WHEN 'P' THEN 'CUOTA FIJA (METODO FRANCES). VALOR CUOTA: '||(SELECT t.total FROM mks_creditos.tabla_amortizacion t WHERE t.numero_credito=lcs.codigo and t.cuota=1)
        END                                                            "TIPO TABLA",
        pen.fecha_nacimiento                                           "FECHA NACIMIENTO",
        per.correo_electronico                                         "CORREO",
        coy.cod_per_con          "COD PER CON",
        CASE WHEN (SELECT pcm.firma_conyuge 
                   FROM   mks_Creditos.Producto_Credito_Moneda_Ifip pcm
                   WHERE  pcm.codigo_producto = lc.codigo_producto 
                   AND    pcm.codigo_moneda = lc.codigo_moneda
                   AND    pcm.codigo_ifip = lcs.codigo_ifip) = 'S' THEN      
                   (SELECT p.nombre_completo 
                    FROM   mks_socios.persona p, 
                           mks_socios.persona_conyuge c
                    WHERE  p.codigo=c.codigo_persona_conyuge 
                    AND     c.codigo_persona=per.codigo
                    AND  c.firma ='S' 
                    AND c.eliminado = 'N') 
                    ELSE NULL END                                      "NOMBRE CON FIR",
        CASE WHEN (SELECT pcm.firma_conyuge 
                   FROM   mks_Creditos.Producto_Credito_Moneda_Ifip pcm
                   WHERE  pcm.codigo_producto = lc.codigo_producto 
                   AND    pcm.codigo_moneda = lc.codigo_moneda
                   AND    pcm.codigo_ifip = lc.codigo_ifip) = 'S' 
           AND(SELECT p.nombre_completo 
                    FROM   mks_socios.persona p, 
                           mks_socios.persona_conyuge c
                    WHERE  p.codigo=c.codigo_persona_conyuge 
                    AND     c.codigo_persona=per.codigo
                    AND  c.firma ='S' 
                    AND c.eliminado = 'N') IS NOT NULL THEN      
                   '---------------------------------------------------------------------------------------------------------------------------------' 
                    ELSE NULL END                                      "GUION FIRMA CON",
         CASE WHEN (SELECT pcm.firma_conyuge 
                   FROM   mks_Creditos.Producto_Credito_Moneda_Ifip pcm
                   WHERE  pcm.codigo_producto = lc.codigo_producto 
                   AND    pcm.codigo_moneda = lc.codigo_moneda
                   AND    pcm.codigo_ifip = lc.codigo_ifip) = 'S' 
                   AND(SELECT p.nombre_completo 
                    FROM   mks_socios.persona p, 
                           mks_socios.persona_conyuge c
                    WHERE  p.codigo=c.codigo_persona_conyuge 
                    AND     c.codigo_persona=per.codigo
                    AND  c.firma ='S' 
                    AND c.eliminado = 'N') IS NOT NULL  THEN      
                   'CONYUGE' 
                    ELSE NULL END                                      "PIE FIRMA CON",  
        CASE WHEN (SELECT pcm.firma_conyuge 
                   FROM   mks_Creditos.Producto_Credito_Moneda_Ifip pcm
                   WHERE  pcm.codigo_producto = lc.codigo_producto 
                   AND    pcm.codigo_moneda = lc.codigo_moneda
                   AND    pcm.codigo_ifip = lc.codigo_ifip) = 'S' THEN      
                   (SELECT p.identificacion 
                    FROM   mks_socios.persona p, 
                           mks_socios.persona_conyuge c
                    WHERE  p.codigo=c.codigo_persona_conyuge 
                    AND     c.codigo_persona=per.codigo
                    AND  c.firma ='S' 
                    AND c.eliminado = 'N') 
                    ELSE NULL END                                     "IDENTIFICACION CON",
                    ssp.total_patrimonio                               "PATRIMONIO",
                    sfc.cobertura                                      "COBERTURA"                                                                        
FROM    mks_creditos.linea_credito_solicitud                           lcs,
        mks_creditos.linea_credito                                     lc,
        mks_creditos.producto_credito                                  pro,
        mks_creditos.producto_credito_moneda                           prm,
        mks_creditos.producto_credito_moneda_ifip                      pri,
        mks_socios.actividad_economica                                 act,
        mks_socios.socio                                               soc,
        mks_socios.persona                                             per,
        mks_socios.persona_residencia                                  pes,        
        mks_socios.persona_natural                                     pen,
        mks_socios.persona_act_eco_pri                                 atp,
        mks_socios.actividad_economica                                 aep,
        mks_comunes.periodicidad                                       prd,
        mks_ifips.ifip                                                 ifi,
        mks_ifips.tipo_ifip                                            tii,
        mks_ifips.ifip_agencia                                         ifa,
        mks_comunes.ubicacion_geografica                               ubg,
        mks_comunes.moneda                                             mon,
        mks_creditos.tipo_cartera                                      tic,
        mks_socios.tipo_identificacion                                 tid,
        mks_socios.estado_civil                                        esc,
        mks_socios.instruccion                                         ins,
        mks_socios.profesion                                           prf,
        conyuge                                                        coy,
        mks_socios.socio_flujo_caja                                    sfc,
        mks_socios.socio_situacion_patrimonial                         ssp
WHERE   lcs.codigo                                                     = $P{codigoLineaCredito}
AND     lcs.codigo_ifip                                                = $P{codigoIfip}
AND     pro.codigo                                                     = lc.codigo_producto
AND     prm.codigo_producto                                            = lc.codigo_producto
AND     prm.codigo_moneda                                              = lc.codigo_moneda
AND     pri.codigo_producto                                            = lc.codigo_producto
AND     pri.codigo_moneda                                              = lc.codigo_moneda
AND     pri.codigo_ifip                                                = lcs.codigo_ifip
AND     act.codigo                                                     = lcs.codigo_act_eco
AND     soc.codigo                                                     = lcs.codigo_socio
AND     soc.codigo_ifip                                                = lcs.codigo_ifip
AND     soc.codigo_persona                                             = per.codigo
AND     per.codigo                                                     = pes.codigo_persona
AND     per.codigo                                                     = pen.codigo_persona
AND     per.codigo                                                     = atp.codigo_persona
AND     aep.codigo                                                     = atp.codigo_actividad_economica
AND     ifi.codigo                                                     = lcs.codigo_ifip
AND     ifa.codigo                                                     = lcs.codigo_ifip_agencia
AND     tii.codigo                                                     = ifi.codigo_tipo_ifip
AND     lcs.codigo_linea_credito                                       = lc.codigo
AND     lc.codigo_moneda                                               = mon.codigo
AND     lc.codigo_periodicidad                                         = prd.codigo
AND     ubg.codigo                                                     = ifa.codigo_ubi_geo_ciudad
AND     pro.codigo_tipo_cartera                                        = tic.codigo
AND     per.codigo_tipo_identificacion                                 = tid.codigo
AND     pen.codigo_instruccion                                         = ins.codigo
AND     pen.codigo_estado_civil                                        = esc.codigo
AND     pen.codigo_profesion                                           = prf.codigo
AND     soc.codigo_persona                                              = coy.codigo_persona (+)
AND     sfc.codigo_socio                                               = soc.codigo
AND     sfc.codigo_ifip                                                = soc.codigo_ifip
AND     ssp.codigo_socio                                               = soc.codigo
AND     ssp.codigo_ifip                                                = soc.codigo_ifip]]>
	</queryString>
