	<subDataset name="FlujoCajaIng" uuid="ecee45cf-2e3f-4a5b-b0e8-47fc8ba804b6">
		<parameter name="codIfi" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="codSoc" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT ifc.nombre,sfc.total_socio,sfc.total_conyuge 
FROM mks_socios.socio_flujo_caja_ingreso sfc,
              mks_socios.item_flujo_caja ifc
WHERE         sfc.codigo_item_flu_caj    =ifc.codigo
AND           sfc.codigo_socio           =$P{codSoc}
AND           sfc.codigo_ifip            = $P{codIfi}
AND           (sfc.total_conyuge          >0 OR sfc.total_socio>0)]]>
		</queryString>
		<field name="NOMBRE" class="java.lang.String"/>
		<field name="TOTAL_SOCIO" class="java.math.BigDecimal"/>
		<field name="TOTAL_CONYUGE" class="java.math.BigDecimal"/>
		<variable name="totIngSoc" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TOTAL_SOCIO}]]></variableExpression>
		</variable>
		<variable name="totIngCon" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TOTAL_CONYUGE}]]></variableExpression>
		</variable>
	</subDataset>
