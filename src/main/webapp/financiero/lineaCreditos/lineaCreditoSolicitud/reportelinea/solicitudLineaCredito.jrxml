	<subDataset name="diasNotificacion" uuid="ceff97fa-e59d-48c8-bdf3-2b307e28e487">
		<parameter name="codIfip" class="java.lang.Long"/>
		<queryString>
			<![CDATA[select deni.dia_inicial||' a '||deni.dia_final DIAS,
       tn.nombre NOTIFICACION,
       tni.valor VALOR
from mks_creditos.tipo_notificacion tn,
     mks_creditos.tipo_notificacion_ifip tni,
     mks_creditos.dias_envio_notificacion_ifip deni,
     mks_ifips.ifip ifi
where tn.codigo=tni.codigo_tipo_notificacion
and   tni.codigo_tipo_notificacion=deni.codigo_tipo_notificacion 
and   tni.codigo_ifip=ifi.codigo
and   deni.codigo_ifip=ifi.codigo
and   ifi.codigo=$P{codIfip}]]>
		</queryString>
		<field name="DIAS" class="java.lang.String"/>
		<field name="NOTIFICACION" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="telefonosSocio" uuid="8696ed09-f960-4594-8011-6b949b7fad82">
		<parameter name="codSoc" class="java.math.BigDecimal"/>
		<parameter name="codIfi" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT  tip.nombre                      "TIPO TELEFONO",
        opt.siglas                      "OPERADORA",
        pet.numero                      "NUMERO"
FROM	  mks_comunes.tipo_telefono 	     tip,
        mks_comunes.operadora_telefono   opt,
        mks_socios.socio	               soc,
        mks_socios.persona_telefono      pet
WHERE   soc.codigo                       = $P{codSoc}
AND     soc.codigo_ifip                  = $P{codIfi}
AND     pet.eliminado                    = 'N'
AND     pet.codigo_persona               = soc.codigo_persona
AND     tip.codigo                       = pet.codigo_tipo_telefono
AND     opt.codigo                       = pet.codigo_operadora_telefono
AND     rownum			<= 2
ORDER BY tip.nombre, pet.numero]]>
		</queryString>
		<field name="TIPO TELEFONO" class="java.lang.String"/>
		<field name="OPERADORA" class="java.lang.String"/>
		<field name="NUMERO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="trabajos" uuid="a3107bcc-df63-46f6-be6d-3ee16e0e08da">
		<parameter name="codSoc" class="java.math.BigDecimal"/>
		<parameter name="codIfi" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT act.nombre                            "ACTVIDAD",
       emp.nombre                            "EMPRESA",
       car.nombre                            "CARGO",
       pat.tiempo||' - '||ped.nombre           "TIEMPO",
       pat.direccion                         "DIRECCION",
       tit.nombre                            "TIPO TELEFONO",
       pat.numero_telefonico                 "TELEFONO"
FROM   mks_socios.socio                      soc,
       mks_socios.persona                    per,
       mks_socios.persona_trabajo_act_eco    pat,
       mks_socios.actividad_economica       act,
       mks_socios.cargo                     car,
       mks_comunes.tipo_telefono            tit,
       mks_comunes.periodicidad             ped,
       mks_comunes.empresa                  emp,
       mks_socios.persona_act_eco_pri       pap
WHERE  soc.codigo                           = $P{codSoc}
AND    soc.codigo_ifip                      = $P{codIfi}
AND    soc.codigo_persona                   = per.codigo
AND    pat.codigo_persona                   = per.codigo
AND    pat.codigo_actividad_economica       = act.codigo
AND    pat.codigo_cargo                     = car.codigo
AND    pat.codigo_empresa                   = emp.codigo
AND    pat.codigo_tipo_telefono             = tit.codigo
AND    ped.codigo                           = pat.codigo_periodicidad
AND    pap.codigo_persona                   = per.codigo
ORDER BY  pat.codigo]]>
		</queryString>
		<field name="ACTVIDAD" class="java.lang.String"/>
		<field name="EMPRESA" class="java.lang.String"/>
		<field name="CARGO" class="java.lang.String"/>
		<field name="TIEMPO" class="java.lang.String"/>
		<field name="DIRECCION" class="java.lang.String"/>
		<field name="TIPO TELEFONO" class="java.lang.String"/>
		<field name="TELEFONO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="mora" uuid="6a485034-008a-4eb2-9ae7-aa6daa5ae3b3">
		<parameter name="codTipCar" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT DECODE(dia_inicia,61,'MAYOR O IGUAL ',TO_CHAR(dia_inicia))||' a '||DECODE(dia_fin,9999,TO_CHAR(dia_inicia),TO_CHAR(dia_fin)) DIAS,
        interes_mora porcentaje
FROM   mks_creditos.tasa_mora
WHERE  codigo_tipo_cartera         = $P{codTipCar}
ORDER BY dia_inicia]]>
		</queryString>
		<field name="DIAS" class="java.lang.String"/>
		<field name="PORCENTAJE" class="java.math.BigDecimal"/>
	</subDataset>
