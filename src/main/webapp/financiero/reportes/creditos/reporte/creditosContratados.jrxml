	<subDataset name="Detalle" uuid="3d80251e-0e4e-4b18-aa7d-aba9eb50b3e9">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="fecFin" class="java.util.Date"/>
		<parameter name="fecInicio" class="java.util.Date"/>
		<parameter name="codEstado" class="java.lang.Long"/>
		<parameter name="codigoIfipAgencia" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT sol.numero                   numero,
       per.nombre_completo          nombre,
       sol.monto_credito            monto,
       sol.fecha_solicitud          fecha,
       est.nombre                   estado
FROM   mks_creditos.solicitud         sol,
       mks_creditos.estado_credito    est,
       mks_socios.socio               soc,
       mks_socios.persona             per
WHERE  TRUNC(sol.fecha_solicitud)     >= TRUNC($P{fecInicio})
AND    TRUNC(sol.fecha_solicitud)     <= TRUNC($P{fecFin}) 
AND    sol.codigo_ifip                =$P{codIfip}
AND    sol.codigo_estado              =$P{codEstado}
AND    sol.codigo_ifip_agencia	       = $P{codigoIfipAgencia}
AND    sol.codigo_estado              =est.codigo
AND    sol.codigo_socio               =soc.codigo
AND    soc.codigo_persona             =per.codigo]]>
		</queryString>
		<field name="NUMERO" class="java.math.BigDecimal"/>
		<field name="NOMBRE" class="java.lang.String"/>
		<field name="MONTO" class="java.math.BigDecimal"/>
		<field name="FECHA" class="java.sql.Timestamp"/>
		<field name="ESTADO" class="java.lang.String"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/vicastoc/renafipse/unidad-tecnlogia/minkasoftware/desarrollo/minkasoftware/src/main/webapp/financiero/socios/socio/reporte/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoIfip" class="java.lang.Long"/>
	<parameter name="fechaInicio" class="java.util.Date"/>
	<parameter name="fechaFin" class="java.util.Date"/>
	<parameter name="codigoEstado" class="java.lang.Long"/>
	<parameter name="codigoIfipAgencia" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT  tti.nombre||' '||ifi.nombre           "IFIP",
        ifi.path_imagen                       "IMAGEN",
        est.nombre			     "ESTADO",
        ifa.nombre			     "AGENCIA"

FROM    mks_ifips.ifip                         ifi,
        mks_ifips.tipo_ifip                    tti,
        mks_creditos.estado_credito	      est	,
        mks_ifips.ifip_agencia	                ifa
WHERE   ifi.codigo                             = $P{codigoIfip}
AND     ifa.codigo			      = $P{codigoIfipAgencia}
AND     tti.codigo                             = ifi.codigo_tipo_ifip
AND     est.codigo			      =$P{codigoEstado}
AND     ifa.codigo_ifip   	                =  ifi.codigo]]>
	</queryString>
	<field name="IFIP" class="java.lang.String"/>
	<field name="IMAGEN" class="java.lang.String"/>
	<field name="ESTADO" class="java.lang.String"/>
	<field name="AGENCIA" class="java.lang.String"/>
