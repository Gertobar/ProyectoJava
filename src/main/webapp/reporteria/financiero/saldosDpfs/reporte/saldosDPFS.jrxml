	<subDataset name="Detalle" uuid="3d80251e-0e4e-4b18-aa7d-aba9eb50b3e9">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="fecCorte" class="java.util.Date"/>
		<queryString>
			<![CDATA[SELECT  orden_rango orden,
        rango banda,
        SUM(capital) saldo
FROM    (SELECT d.capital, 
                 CASE WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 0 AND 30 THEN '1-30'
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 31 AND 90 THEN '31-90'
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 91 AND 180 THEN '91-180'
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 181 AND 360 THEN '181-360'
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           > 360 THEN '+360' END rango,
                CASE WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 0 AND 30 THEN 1
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 31 AND 90 THEN 2
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 91 AND 180 THEN 3
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           BETWEEN 181 AND 360 THEN 4
                      WHEN trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte})
                           > 360 THEN 5 END orden_rango,
                 d.codigo_ifip,
                 trunc(d.fecha_vencimiento) - TRUNC($P{fecCorte}) diasvence

          FROM   mks_historicos.saldo_dpf d
          WHERE  codigo_ifip                  = $P{codIfip}
          AND    trunc(d.fecha)       	     = TRUNC($P{fecCorte})
          AND    TRUNC(d.fecha_contrato)     <= TRUNC($P{fecCorte}) 
          AND    TRUNC(d.fecha_vencimiento)  > TRUNC($P{fecCorte}) 
          AND    d.estado                    = 'V'
          )
GROUP BY orden_rango, rango
ORDER BY orden_rango, rango]]>
		</queryString>
		<field name="ORDEN" class="java.math.BigDecimal"/>
		<field name="BANDA" class="java.lang.String"/>
		<field name="SALDO" class="java.math.BigDecimal"/>
	</subDataset>
