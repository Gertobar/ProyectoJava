	<subDataset name="Detalle" uuid="3d80251e-0e4e-4b18-aa7d-aba9eb50b3e9">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="fecCorte" class="java.util.Date"/>
		<parameter name="fecInicio" class="java.util.Date"/>
		<parameter name="per" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT  (SELECT nombre
         FROM   mks_contables.plan_de_cuenta
         WHERE  cuenta_contable                
                   = (SELECT   cuenta_padre
                     FROM     mks_contables.plan_de_cuenta
                     WHERE    cuenta_contable = pla.cuenta_padre  
                     AND      codigo_tipo_plan=pla.codigo_tipo_plan)
         AND    codigo_tipo_plan=pla.codigo_tipo_plan)                            "GRUPO",
        pla.nombre                                                                "NOMBRE CUENTA",
        pla.cuenta_contable                                                       "CUENTA CONTABLE",
        debito-credito                                                            "SALDO"
FROM    (SELECT  SUM(DECODE(cod.tipo,'C',cod.valor,0)) credito,
                  SUM(DECODE(cod.tipo,'D',cod.valor,0)) debito,
                  cod.codigo_tipo_plan                  codigo_tipo_plan,
                  cod.cuenta_contable                   cuenta_contable
          FROM    mks_contables.comprobante                 com,
                  mks_contables.comprobante_detalle         cod
          WHERE   com.codigo_periodo                        = $P{per}
          AND     com.codigo_ifip                           = $P{codIfip}
          AND     cod.cuenta_contable                       LIKE '14%'
          AND     cod.cuenta_contable                       NOT LIKE '1499%'
          AND     TRUNC(com.fecha)                          >= TRUNC($P{fecInicio})
          AND     TRUNC(com.fecha)                          <= TRUNC($P{fecCorte})
          AND     com.codigo                                = cod.codigo_comprobante
          AND     com.codigo_estado                         <>4
          GROUP BY cod.cuenta_contable, cod.codigo_tipo_plan)                     con,
          mks_contables.plan_de_cuenta                                            pla,
          mks_contables.tipo_plan_de_cuenta                                       tpc
WHERE     con.codigo_tipo_plan                                                    = pla.codigo_tipo_plan
AND       con.cuenta_contable                                                     = pla.cuenta_contable
AND       tpc.codigo                                                              = pla.codigo_tipo_plan
AND       trunc(tpc.fecha_inicio_vigencia)                                        <=TRUNC($P{fecCorte})    
AND       (trunc(tpc.fecha_fin_vigencia)                                          is null
          OR
          trunc(tpc.fecha_fin_vigencia)                                           >=TRUNC($P{fecCorte}))

--AND       debito-credito                                                          > 0
ORDER BY  pla.cuenta_contable]]>
		</queryString>
		<field name="GRUPO" class="java.lang.String"/>
		<field name="NOMBRE CUENTA" class="java.lang.String"/>
		<field name="CUENTA CONTABLE" class="java.lang.String"/>
		<field name="SALDO" class="java.math.BigDecimal"/>
	</subDataset>
