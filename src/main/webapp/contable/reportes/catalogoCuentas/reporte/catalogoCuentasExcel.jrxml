	<subDataset name="Detalle" uuid="3d80251e-0e4e-4b18-aa7d-aba9eb50b3e9">
		<parameter name="cod" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT  plc.cuenta_contable                    "CUENTA",
        plc.nombre                             "NOMBRE",
        gpc.nombre                             "GRUPO",
        CASE WHEN plc.es_de_movimiento = 'S' THEN 'SI' ELSE 'NO' END     "DE MOVIMIENTO",        
        plc.nivel                              "NIVEL"
FROM    mks_contables.plan_de_cuenta_ifip      pci,
        mks_contables.plan_de_cuenta           plc,
        mks_contables.grupo_cuenta_contable    gpc               
WHERE   pci.codigo_ifip                        = $P{cod}
AND     pci.cuenta_contable                    =plc.cuenta_contable
AND     pci.codigo_tipo_plan                   =plc.codigo_tipo_plan
AND     plc.codigo_grupo                       =gpc.codigo
ORDER BY plc.cuenta_contable]]>
		</queryString>
		<field name="CUENTA" class="java.lang.String"/>
		<field name="NOMBRE" class="java.lang.String"/>
		<field name="GRUPO" class="java.lang.String"/>
		<field name="DE MOVIMIENTO" class="java.lang.String"/>
		<field name="NIVEL" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/vicastoc/renafipse/unidad-tecnlogia/minkasoftware/desarrollo/minkasoftware/src/main/webapp/financiero/socios/socio/reporte/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigo" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT  tti.nombre||' '||ifi.nombre           "IFIP",
        ifi.path_imagen                       "IMAGEN"

FROM    mks_ifips.ifip                         ifi,
        mks_ifips.tipo_ifip                    tti
WHERE   ifi.codigo                             = $P{codigo}
AND     tti.codigo                             = ifi.codigo_tipo_ifip]]>
	</queryString>
	<field name="IFIP" class="java.lang.String"/>
