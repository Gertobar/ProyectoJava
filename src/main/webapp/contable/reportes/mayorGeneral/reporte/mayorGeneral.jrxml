	<subDataset name="mayorGeneral" uuid="7dbcb560-d1d5-4f04-b3d4-457627d02204">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="fecInicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="fecFin" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="cueCont" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="codigoTipoPlan" class="java.lang.Long"/>
		<parameter name="codPer" class="java.lang.String"/>
		<parameter name="saldoInicial" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT c.*,
        case when rownum=1 then
        $P{saldoInicial}
            ELSE 0
            END SALDOINICIAL
FROM(
SELECT  com.numero_comprobante                "NUMERO COMPROBANTE",
        com.codigo                            "CODIGO",
        com.glosa                             "GLOSA",
        com.fecha                             "FECHA",
        CASE WHEN cdt.tipo = 'C' THEN -1*cdt.valor ELSE 0 END  "CREDITO",
        CASE WHEN cdt.tipo = 'D' THEN cdt.valor ELSE 0 END     "DEBITO"
FROM    mks_contables.comprobante             com,
        mks_contables.comprobante_detalle     cdt
WHERE   com.codigo_ifip                       =$P{codIfip}
AND     cdt.cuenta_contable                   =$P{cueCont}
AND     TRUNC(com.fecha)                      >= trunc($P{fecInicio})
AND     TRUNC(com.fecha)                      <= trunc($P{fecFin})
AND     com.codigo_estado        = 3
AND     cdt.codigo_tipo_plan        = $P{codigoTipoPlan}
AND     com.codigo                            =cdt.codigo_comprobante
AND     com.codigo_estado                     <>4
ORDER BY             com.fecha) c]]>
		</queryString>
		<field name="NUMERO COMPROBANTE" class="java.lang.String"/>
		<field name="CODIGO" class="java.math.BigDecimal"/>
		<field name="GLOSA" class="java.lang.String"/>
		<field name="FECHA" class="java.sql.Timestamp"/>
		<field name="CREDITO" class="java.math.BigDecimal"/>
		<field name="DEBITO" class="java.math.BigDecimal"/>
		<field name="SALDOINICIAL" class="java.math.BigDecimal"/>
		<variable name="saldo" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{CREDITO}.add( $F{DEBITO}).add($F{SALDOINICIAL})]]></variableExpression>
			<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
		</variable>
		<variable name="totalCredito" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{CREDITO}]]></variableExpression>
			<initialValueExpression><![CDATA[]]></initialValueExpression>
		</variable>
		<variable name="totalDebito" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{DEBITO}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/vicastoc/renafipse/unidad-tecnlogia/minkasoftware/desarrollo/minkasoftware/src/main/webapp/financiero/socios/socio/reporte/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoIfip" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaInicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaFin" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="cuentaContable" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoTipoPlan" class="java.lang.Long"/>
	<parameter name="codigoPeriodo" class="java.lang.String"/>
	<queryString>
		<![CDATA[WITH det AS (SELECT  CASE WHEN cdt.tipo = 'C' THEN cdt.valor ELSE 0 END     credito,
                     CASE WHEN cdt.tipo = 'D' THEN cdt.valor ELSE 0 END     debito
FROM    mks_contables.comprobante             com,
        mks_contables.comprobante_detalle     cdt
WHERE   com.codigo_ifip                       =$P{codigoIfip}
AND     cdt.cuenta_contable                   =$P{cuentaContable}
AND     TRUNC(com.fecha)                      >= $P{fechaInicio}
AND     TRUNC(com.fecha)                      <= $P{fechaFin}
AND     cdt.codigo_tipo_plan		     = $P{codigoTipoPlan}
AND    com.codigo_periodo         	     = $P{codigoPeriodo}
AND     com.codigo                            =cdt.codigo_comprobante
AND     com.codigo_estado		     <>4)


SELECT  tti.nombre||' '||ifi.nombre           "IFIP",
        ifi.path_imagen                       "IMAGEN",
        plc.nombre                            "CUENTA",
        (SELECT (sum(det.debito))
                FROM
                det)                          "TOTAL DEBITO",
        (SELECT (sum(det.credito)*-1)
                FROM
                det)                          "TOTAL CREDITO",
        (select nvl(
                   sum(
                   case dt.tipo
                   WHEN 'D' THEN dt.valor
                   ELSE dt.valor*-1
                   END),0)
            from   mks_contables.comprobante_detalle dt,
                   mks_contables.comprobante cm
            where  cm.codigo=dt.codigo_comprobante
            AND    dt.codigo_tipo_plan=$P{codigoTipoPlan}
            AND    dt.cuenta_contable=$P{cuentaContable}
            AND    cm.codigo_periodo         =$P{codigoPeriodo}
	  AND    cm.codigo_estado<>4
            AND    trunc(cm.fecha)<trunc($P{fechaInicio}
	 ))+
        (SELECT (sum(det.debito)-sum(det.credito))
                FROM
                det)                          "SALDO",
        (select nvl(
                   sum(
                   case dt.tipo
                   WHEN 'D' THEN dt.valor
                   ELSE dt.valor*-1
                   END),0)
            from   mks_contables.comprobante_detalle dt,
                   mks_contables.comprobante cm
            where  cm.codigo=dt.codigo_comprobante
            AND    dt.codigo_tipo_plan=$P{codigoTipoPlan}
            AND    dt.cuenta_contable=$P{cuentaContable}
            AND    cm.codigo_periodo         =$P{codigoPeriodo}
	  AND    cm.codigo_estado<>4
            AND    trunc(cm.fecha)<trunc($P{fechaInicio})) SALDOINICIAL
FROM    mks_ifips.ifip                         ifi,
        mks_ifips.tipo_ifip                    tti,
        mks_contables.plan_de_cuenta           plc
WHERE   ifi.codigo                             =$P{codigoIfip}
AND     plc.cuenta_contable                    =$P{cuentaContable}
AND     tti.codigo                             =ifi.codigo_tipo_ifip
AND     plc.codigo_tipo_plan                   =$P{codigoTipoPlan}]]>
	</queryString>
