<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TipoEntidadFinancieraViewDlg" 
                  widgetVar="TipoEntidadFinancieraViewDialog"
                  modal="true" resizable="false" 
                  appendToBody="true"
                  closeOnEscape="true">
<f:facet name="header">
    <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.TipoEntidadFinanciera}"
                               styleClass="encabezadosDialog"/>
            </f:facet>     

            <h:form id="TipoEntidadFinancieraViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{tipoEntidadFinancieraController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{tipoEntidadFinancieraController.selected.codigo}" title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Nombre}"/>
                        <h:outputText value="#{tipoEntidadFinancieraController.selected.nombre}" title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Siglas}"/>
                        <h:outputText value="#{tipoEntidadFinancieraController.selected.siglas}" title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                         <p:selectOneMenu id="eliminado" 
                                          value="#{tipoEntidadFinancieraController.selected.eliminado}" 
                                         disabled="true"> 
                             <f:selectItem itemLabel="#{propiedadesEtiquetaBL.No}" itemValue="N" />  
                             <f:selectItem itemLabel="#{propiedadesEtiquetaBL.Si}" itemValue="S" />                          
                        </p:selectOneMenu>
                    </p:panelGrid>
                 
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
