	<subDataset name="rubros" uuid="afee9ca4-7b2c-419b-a16d-4eabf82a0ee6">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="numCre" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT tru.nombre rubro,
       DECODE(tr.calcula_sobre,'M','MONTO DEL AVANCE','S','SALDO CAPITAL DEL CUPO CONSUMIDO') calculado_sobre,
       DECODE(tr.cobrado_en,'C','COBRADO AL MOMENTO DEL AVANCE','D','DISTRIBUIDO EN LOS PAGOS DE LÍNEA DE CREDITO') cobrado_en,
       TRIM(TO_CHAR(ROUND(tr.valor,2),'9999999990D99'))||CASE WHEN tr.tipo = 'P' THEN '%' ELSE '' END valor
FROM   mks_creditos.linea_credito_solicitud lcs,
       mks_creditos.linea_credito lc,
       mks_creditos.producto_credito_moneda_ifip pcmi,
       mks_creditos.tipo_rubro_pro_cre_mon_ifi tr,
       mks_creditos.tipo_rubro tru
WHERE  lcs.codigo_linea_credito = lc.codigo
AND    lc.codigo_producto = pcmi.codigo_producto
AND    lc.codigo_moneda = pcmi.codigo_moneda
AND    lc.codigo_ifip = pcmi.codigo_ifip
AND    tr.codigo_producto = pcmi.codigo_producto
AND    tr.codigo_moneda = pcmi.codigo_moneda
AND    tr.codigo_ifip = pcmi.codigo_ifip
AND    tru.codigo = tr.codigo_tipo_rubro
AND    lcs.codigo = $P{numCre}
AND    tru.codigo != 8
ORDER BY tr.orden_cobro]]>
		</queryString>
		<field name="RUBRO" class="java.lang.String"/>
		<field name="CALCULADO_SOBRE" class="java.lang.String"/>
		<field name="COBRADO_EN" class="java.lang.String"/>
		<field name="VALOR" class="java.lang.String"/>
	</subDataset>
	<subDataset name="cuentas" uuid="81a12e59-49b3-4802-96da-a158a155734e">
		<parameter name="codSoc" class="java.math.BigDecimal"/>
		<parameter name="codIfi" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT  mon.nombre                  moneda,
        tip.nombre                  producto,
        cue.numero                  numero,
        cue.saldo_total             saldo
        
FROM    mks_ahorros.cuenta            cue,
        mks_ahorros.tipo_producto     tip,
        mks_comunes.moneda            mon,
        mks_ahorros.estado_cuenta     est
WHERE   codigo_socio                  = $P{codSoc}
AND     codigo_ifip                   = $P{codIfi}
AND     est.puede_reactivar           = 'S'
AND     est.codigo                    = cue.codigo_estado
AND     tip.codigo                    = cue.codigo_tipo_producto
AND     mon.codigo                    = cue.codigo_moneda
ORDER BY tip.codigo, cue.numero]]>
		</queryString>
		<field name="MONEDA" class="java.lang.String"/>
		<field name="PRODUCTO" class="java.lang.String"/>
		<field name="NUMERO" class="java.lang.String"/>
		<field name="SALDO" class="java.math.BigDecimal"/>
		<variable name="total" class="java.lang.String" calculation="Sum">
			<variableExpression><![CDATA[$F{SALDO}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="creditos" uuid="295b11f9-16fc-4d9c-8f89-af475556f552">
		<parameter name="codSoc" class="java.math.BigDecimal"/>
		<parameter name="codIfi" class="java.lang.Long"/>
		<parameter name="numCre" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT   sol.numero                                         numero_credito,
         sol.fecha_solicitud                                fecha_solicitud,
         sol.monto_credito                                  monto_solicitado,
         sol.tasa                                           tasa,
         ped.nombre                                         periodicidad,
         CASE esc.codigo
         WHEN 6 THEN esc.nombre ||' ('||(select nvl(max(t.cuota),0) from mks_creditos.tabla_amortizacion t where t.numero_credito=sol.numero and t.estado='C')||' de '||sol.numero_cuotas||' cuotas canceladas)'
         ELSE        esc.nombre
         END                                         estado,
         (SELECT round(avg(h.dias_mora),2) 
         FROM mks_historicos.tablas_creditos h 
         WHERE h.numero_credito=sol.numero)          promedio_mora                                            
FROM     mks_creditos.solicitud                             sol,
         mks_creditos.estado_credito                        esc,
         mks_comunes.periodicidad                           ped,
         mks_creditos.producto_credito                      pro,
         mks_creditos.tipo_cartera                          tic,
         mks_creditos.solicitud_detalle                     det                    
WHERE    sol.codigo_socio                                   = $P{codSoc}
AND      sol.codigo_ifip                                    = $P{codIfi}
AND      sol.codigo_estado                                  = esc.codigo
AND      sol.codigo_periodicidad                            = ped.codigo
AND      pro.codigo                                         = sol.codigo_producto
AND      tic.codigo                                         = pro.codigo_tipo_cartera
AND      det.codigo_ifip                                    = sol.codigo_ifip
AND      det.numero_credito                                 = sol.numero
AND      sol.numero				         <> $P{numCre}
AND      esc.codigo                                         IN (6,7)]]>
		</queryString>
		<field name="NUMERO_CREDITO" class="java.math.BigDecimal"/>
		<field name="FECHA_SOLICITUD" class="java.sql.Timestamp"/>
		<field name="MONTO_SOLICITADO" class="java.math.BigDecimal"/>
		<field name="TASA" class="java.math.BigDecimal"/>
		<field name="PERIODICIDAD" class="java.lang.String"/>
		<field name="ESTADO" class="java.lang.String"/>
		<field name="PROMEDIO_MORA" class="java.math.BigDecimal"/>
		<variable name="total" class="java.lang.String" calculation="Sum">
			<variableExpression><![CDATA[$F{MONTO_SOLICITADO}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="codigoLineaCredito" class="java.lang.Long"/>
	<parameter name="codigoIfip" class="java.lang.Long"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="subReportGarante" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  lcs.codigo                                                     "SOLICITUD N°",
        soc.codigo                                                     "SOCIO No:",
        per.nombre_completo                                            "NOMBRES COMPLETOS",
        per.identificacion                                             "IDENTIFICACION",
        tii.nombre||' '||ifi.nombre                                    "IFIP",
        lcs.codigo                                                     "CREDITO No:" , 
        tic.nombre                                                     "TIPO CARTERA",
        pro.nombre                                                     "PRODUCTO",
        mon.nombre                                                     "MONEDA",
        lcs.monto_linea_credito                                        "MONTO CREDITO",
        act.nombre                                                     "DESTINO CREDITO",
        NVL((SELECT de.nombre 
             FROM mks_creditos.destino_especifico de 
             WHERE lcs.codigo_destino_especifico = de.codigo), lcs.observaciones) "DESTINO ESPECIFICO",
        prd.nombre                                                     "PERIODICIDAD",
        lcs.observaciones                                              "OBSERVACIONES",
        (SELECT TRIM(TO_CHAR(ROUND(ti.valor,2),'9999999990D99'))||' %'
         FROM   mks_creditos.linea_credito lc1,
                mks_creditos.producto_credito_moneda_ifip pcmi,
                mks_creditos.tasa_interes_pro_cre_mon_ifi tipc,
                mks_creditos.tasa_interes_credito_ifip ti
         WHERE  lc1.codigo_producto = pcmi.codigo_producto
         AND    lc1.codigo_moneda = pcmi.codigo_moneda
         AND    lc1.codigo_ifip = pcmi.codigo_ifip
         AND    pcmi.codigo_producto = tipc.codigo_producto
         AND    pcmi.codigo_moneda = tipc.codigo_moneda
         AND    pcmi.codigo_ifip = tipc.codigo_ifip
         AND    tipc.codigo_tasa = ti.codigo
         AND    lc1.codigo = lc.codigo
         AND    tipc.eliminado = 'N')                                "TASA CREDITO",         
        pro.codigo_tipo_cartera                                        ,
        ubg.nombre                                                     "CIUDAD",
        (SELECT p.nombre_completo usuario_nombre
         FROM   mks_auditorias.linea_credito_solicitud_estado e,
                mks_seguridades.usuario u,
                mks_socios.persona p
         WHERE  e.codigo_usuario = u.codigo
         AND    u.codigo_persona = p.codigo
         AND    e.codigo_linea_credito_sol = lcs.codigo
         AND    e.codigo_linea_cre_est_sol = 2)                        "OFICIAL DE CREDITO",
        DECODE(lcsit.factible,'S','SI','N0')                           "FACIBLE",
        lcsit.recomendacion                                            "DETALLE INFORME",
        (SELECT COUNT(*) 
         FROM   mks_creditos.solicitud
         WHERE  codigo_socio             = soc.codigo
         AND    codigo_ifip              = soc.codigo_ifip)               "NUMERO CREDITOS SOCIO",
        REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 7
                AND     codigo_fragmento                   = 1)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$SOCIO',per.nombre_completo)
                ,'$CODSOC',soc.codigo)
                ,'$OFICIAL ', (SELECT p.nombre_completo usuario_nombre
                               FROM   mks_auditorias.linea_credito_solicitud_estado e,
                                      mks_seguridades.usuario u,
                                      mks_socios.persona p
                               WHERE  e.codigo_usuario = u.codigo
                               AND    u.codigo_persona = p.codigo
                               AND    e.codigo_linea_credito_sol = lcs.codigo
                               AND    e.codigo_linea_cre_est_sol = 2)
                )                                                       "NOTA",
        CASE lc.tipo_tabla
             WHEN 'C' THEN 'CAPITAL FIJO (METODO ALEMAN). VALOR PRIMERA CUOTA: '||(SELECT t.total FROM mks_creditos.tabla_amortizacion t WHERE t.numero_credito=lcs.codigo and t.cuota=1)
             WHEN 'P' THEN 'CUOTA FIJA (METODO FRANCES). VALOR CUOTA: '||(SELECT t.total FROM mks_creditos.tabla_amortizacion t WHERE t.numero_credito=lcs.codigo and t.cuota=1)
        END                                                            "TIPO TABLA",
        (SELECT mks_comunes.pkg_utilitario_fecha.f_formato_fecha_1(lcsit.fecha_registro)
         FROM   dual) fecha_informe
FROM    mks_creditos.linea_credito_solicitud                           lcs,
        mks_creditos.linea_credito                                     lc,
        mks_creditos.producto_credito                                  pro,
        mks_creditos.producto_credito_moneda                           prm,
        mks_creditos.producto_credito_moneda_ifip                      pri,
        mks_socios.actividad_economica                                 act,
        mks_socios.socio                                               soc,
        mks_socios.persona                                             per,
        mks_socios.persona_act_eco_pri                                 atp,
        mks_socios.actividad_economica                                 aep,
        mks_comunes.periodicidad                                       prd,
        mks_ifips.ifip                                                 ifi,
        mks_ifips.tipo_ifip                                            tii,
        mks_ifips.ifip_agencia                                         ifa,
        mks_comunes.ubicacion_geografica                               ubg,
        mks_comunes.moneda                                             mon,
        mks_creditos.tipo_cartera                                      tic,
        mks_creditos.linea_credito_sol_inf_tec                         lcsit
WHERE   lcs.codigo                                                     = $P{codigoLineaCredito}
AND     lcs.codigo_ifip                                                = $P{codigoIfip}
AND     lcs.codigo_linea_credito                                       = lc.codigo
AND     pro.codigo                                                     = lc.codigo_producto
AND     prm.codigo_producto                                            = lc.codigo_producto
AND     prm.codigo_moneda                                              = lc.codigo_moneda
AND     pri.codigo_producto                                            = lc.codigo_producto
AND     pri.codigo_moneda                                              = lc.codigo_moneda
AND     pri.codigo_ifip                                                = lcs.codigo_ifip
AND     act.codigo                                                     = lcs.codigo_act_eco
AND     soc.codigo                                                     = lcs.codigo_socio
AND     soc.codigo_ifip                                                = lcs.codigo_ifip
AND     soc.codigo_persona                                             = per.codigo
AND     per.codigo                                                     = atp.codigo_persona
AND     aep.codigo                                                     = atp.codigo_actividad_economica
AND     lc.codigo_periodicidad                                         = prd.codigo
AND     ifi.codigo                                                     = lcs.codigo_ifip
AND     ifa.codigo                                                     = lcs.codigo_ifip_agencia
AND     tii.codigo                                                     = ifi.codigo_tipo_ifip
AND     lc.codigo_moneda                                               = mon.codigo
AND     ubg.codigo                                                     = ifa.codigo_ubi_geo_ciudad
AND     pro.codigo_tipo_cartera                                        = tic.codigo
AND     lcsit.codigo_linea_credito_sol                                 = lcs.codigo]]>
	</queryString>
