<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="ConceptoTransaccionViewDlg" 
                  widgetVar="ProductoIfitViewDialog" 
                  modal="true" 
                  resizable="false" 
                  appendTo="@(body)" 
                  closeOnEscape="true">

            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.Tasa}"
                               styleClass="encabezadosDialog"/>
            </f:facet>    

            <h:form id="ProductoIfitViewForm">

                <h:panelGroup id="display"  style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{tipoGarantiaProCreMonIfiController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Tasa}"/>
                        <h:outputText value="Producto: #{tipoGarantiaProCreMonIfiController.selected.codigoTasa.productoCreditoMonedaIfip.productoCreditoMoneda.productoCredito.nombre} Moneda:  
                              #{tipoGarantiaProCreMonIfiController.selected.codigoTasa.productoCreditoMonedaIfip.productoCreditoMoneda.moneda.nombre} Tasa: 
                              #{tipoGarantiaProCreMonIfiController.selected.codigoTasa.codigoTasa.valor}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.Tipo}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.codigoTipoGarantia.nombre}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.Numero}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.numero}"/>
                        
                        <h:outputText value="OBLIGATORIO"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.obligatorio}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradaPor}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.usuarioRegistradoPor.username}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.fechaRegistro}"
                                      title="#{propiedadesTituloBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{tipoGarantiaProCreMonIfiController.selected.eliminado}"
                                      title="#{propiedadesTituloBL.Eliminado}"/>                       
                    </p:panelGrid>
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
