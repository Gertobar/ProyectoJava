<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="IngresoEgresoViewDlg" 
                  widgetVar="IngresoEgresoViewDialog" 
                  modal="true" resizable="false"  
                  appendToBody="true" 
                  closeOnEscape="true">
            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.IngresoEgresoMoneda}"
                               styleClass="encabezadosDialog"/>
            </f:facet>  


            <h:form id="IngresoEgresoViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" columnClasses="column" rendered="#{ingresoEgresoController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.codigo}" 
                                      title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Nombre}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.nombre}"
                                      title="#{propiedadesEtiquetaBL.Nombre}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Siglas}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.siglas}" 
                                      title="#{propiedadesEtiquetaBL.Siglas}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Descripcion}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.descripcion}" 
                                      title="#{propiedadesEtiquetaBL.Descripcion}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.AfectaCaja}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.afectaCaja}" 
                                      title="#{propiedadesEtiquetaBL.AfectaCaja}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Contabiliza}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.contabiliza}"
                                      title="#{propiedadesEtiquetaBL.Contabiliza}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.eliminado}"
                                      title="#{propiedadesEtiquetaBL.Eliminado}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.EsIngreso}"/>
                        <h:outputText value="#{ingresoEgresoController.selected.esIngreso}" 
                                      title="#{propiedadesEtiquetaBL.EsIngreso}"/>
                    </p:panelGrid>
                    
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
