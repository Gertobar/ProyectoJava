<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="AhorroTasaInteresIfipViewDlg" 
                  widgetVar="AhorroTasaInteresIfipViewDialog" 
                  modal="true" resizable="false"
               appendTo="@(body)"
                  closeOnEscape="true">

                 <f:facet name="header">
                     <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.AhorroTasaInteresIfip}"
                               styleClass="encabezadosDialog"/>
            </f:facet>  
            <h:form id="AhorroTasaInteresIfipViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{ahorroTasaInteresIfipController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{ahorroTasaInteresIfipController.selected.codigo}" 
                                      title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Ifip}"/>
                        <h:outputText value="#{ahorroTasaInteresIfipController.selected.codigoIfip.nombre.toString()}" 
                                      title="#{propiedadesEtiquetaBL.Ifip}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradaPor}"/>
                        <h:outputText value="#{ahorroTasaInteresIfipController.selected.registradaPor.username}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Valor}"/>
                        <h:outputText value="#{ahorroTasaInteresIfipController.selected.valor}"
                                      title="#{propiedadesEtiquetaBL.Valor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{ahorroTasaInteresIfipController.selected.fechaRegistro}" 
                                      title="#{propiedadesEtiquetaBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                            <p:outputLabel value="#{propiedadesEtiquetaBL.Eliminado}" 
                                       for="eliminado"  styleClass="etiquetaRequerida" />
                        
                        <p:selectOneMenu id="eliminado" 
                                         value="#{tasaInteresProductoIfipController.selected.eliminado}"
                                         required="true"
                                         disabled="true"
                                         requiredMessage="#{propiedadesEtiquetaBL.Eliminado}: #{propiedadesMensajesBL.CampoRequerido}">                          
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.No}" itemValue="N" />  
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.Si}" itemValue="S" />                                  
                        </p:selectOneMenu>
                    </p:panelGrid>
                   
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
