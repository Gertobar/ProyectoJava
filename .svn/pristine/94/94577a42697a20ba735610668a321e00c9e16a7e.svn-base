<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TasaInteresProductoIfipViewDlg"
                  widgetVar="TasaInteresProductoIfipViewDialog"
                  modal="true" resizable="false" 
               appendTo="@(body)"
                  
                  closeOnEscape="true">
            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.TasaInteresProducto}"
                               styleClass="encabezadosDialog"/>
            </f:facet>   


            <h:form id="TasaInteresProductoIfipViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{tasaInteresProductoIfipController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.codigo}"
                                      title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RangoInicial}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.rangoInicial}"
                                      title="#{propiedadesEtiquetaBL.RangoInicial}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RangoFinal}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.rangoFinal}" 
                                      title="#{propiedadesEtiquetaBL.RangoFinal}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradoPor}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.registradoUsuarioPor.username}"
                                      title="#{propiedadesEtiquetaBL.RegistradoPor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.fechaRegistro}" 
                                      title="#{propiedadesEtiquetaBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.ProductoIfip}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.tipoProducto.nombre.toString()}"
                                      title="#{propiedadesEtiquetaBL.ProductoIfip}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Tasa}"/>
                        <h:outputText value="#{tasaInteresProductoIfipController.selected.codigoTasaInteres.valor}"
                                      title="#{propiedadesEtiquetaBL.Tasa}"/>
                        
                        <p:outputLabel value="#{propiedadesEtiquetaBL.Eliminado}" 
                                       for="eliminado"  styleClass="etiquetaRequerida" />
                        
                        <p:selectOneMenu id="eliminado" 
                                         value="#{tasaInteresProductoIfipController.selected.eliminado}"
                                         required="true" 
                                         disabled="true"
                                         requiredMessage="#{propiedadesEtiquetaBL.Eliminado}: #{propiedadesMensajesBL.CampoRequerido}">                          
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.No}" itemValue="N" />  
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.Si}" itemValue="S" />                                  
                        </p:selectOneMenu>
                    </p:panelGrid>
                  
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
