	<subDataset name="Detalle" uuid="3d80251e-0e4e-4b18-aa7d-aba9eb50b3e9">
		<parameter name="codIfip" class="java.lang.Long"/>
		<parameter name="fecCorte" class="java.util.Date"/>
		<queryString>
			<![CDATA[SELECT      fecha, tcf.nombre nombre_cuadre, plc.cuenta_contable, 
            plc.nombre nombre_cuenta, ccc.saldo_contable,
            ccc.saldo_financiero, DECODE(ccc.cuadrado,'S','SI','NO') cuadrado,
            ccc.valor_descuadre diferencia
FROM        mks_contables.cuadre_contable_financiero  ccc,
            mks_contables.plan_de_cuenta              plc,
            mks_contables.cuentas_cuadre_financiero   tcf
WHERE       TRUNC(fecha)                              = TRUNC($P{fecCorte})
AND         tcf.codigo_ifip                           = $P{codIfip}
AND         ccc.codigo_cuadre_cuenta                  = tcf.codigo
AND         plc.cuenta_contable                       = tcf.cuenta_contable
AND         plc.codigo_tipo_plan                      = tcf.codigo_tipo_plan
ORDER BY    plc.cuenta_contable]]>
		</queryString>
		<field name="FECHA" class="java.sql.Timestamp"/>
		<field name="NOMBRE_CUADRE" class="java.lang.String"/>
		<field name="CUENTA_CONTABLE" class="java.lang.String"/>
		<field name="NOMBRE_CUENTA" class="java.lang.String"/>
		<field name="SALDO_CONTABLE" class="java.math.BigDecimal"/>
		<field name="SALDO_FINANCIERO" class="java.math.BigDecimal"/>
		<field name="CUADRADO" class="java.lang.String"/>
		<field name="DIFERENCIA" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/vicastoc/renafipse/unidad-tecnlogia/minkasoftware/desarrollo/minkasoftware/src/main/webapp/financiero/socios/socio/reporte/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoIfip" class="java.lang.Long"/>
	<parameter name="fechaCorte" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT  tti.nombre||' '||ifi.nombre           "IFIP",
        ifi.path_imagen                       "IMAGEN"

FROM    mks_ifips.ifip                         ifi,
        mks_ifips.tipo_ifip                    tti
WHERE   ifi.codigo                             = $P{codigoIfip}
AND     tti.codigo                             = ifi.codigo_tipo_ifip]]>
	</queryString>
