<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TasaInteresDpfIfipViewDlg" 
                  widgetVar="TasaInteresDpfIfipViewDialog"
                  modal="true" resizable="false"  appendToBody="true"
                   closeOnEscape="true">
            
                 <f:facet name="header">
                     <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.TasaInteresDpfIfip}"
                               styleClass="encabezadosDialog"/>
                   </f:facet>
                               

            <h:form id="TasaInteresDpfIfipViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" columnClasses="column" rendered="#{tasaInteresDpfIfipController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.codigo}"
                                      title="#{propiedadesEtiquetaBL.Codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Ifip}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.codigoIfip}" 
                                      title="#{propiedadesEtiquetaBL.Ifip}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Valor}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.valor}"
                                      title="#{propiedadesEtiquetaBL.Valor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradoPor}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.registradoPor}" 
                                      title="#{propiedadesEtiquetaBL.RegistradoPor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.fechaRegistro}" 
                                      title="#{propiedadesEtiquetaBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{tasaInteresDpfIfipController.selected.eliminado}" 
                                      title="#{propiedadesEtiquetaBL.Eliminado}"/>
                    </p:panelGrid>
                   
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
