<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:p="http://primefaces.org/ui">

    <h:form id="ProtestoChequeDetalleListForm">

        <p:panel header="#{myBundle.ListProtestoChequeDetalleTitle}">

            <p:dataTable id="datalist" value="#{protestoChequeDetalleController.items}" var="item"
                         selectionMode="single" selection="#{protestoChequeDetalleController.selected}"
                         rowKey="#{item.protestoChequeDetallePK}"
                         paginator="true"
                         rows="10"
                         rowsPerPageTemplate="10,20,30"
                         >

                <p:ajax event="rowSelect"   update="createButton viewButton editButton deleteButton"/>
                <p:ajax event="rowUnselect" update="createButton viewButton editButton deleteButton"/>

                <p:column sortBy="#{item.protestoChequeDetallePK.codigoMoneda}" filterBy="#{item.protestoChequeDetallePK.codigoMoneda}">
                    <f:facet name="header">
                        <h:outputText value="#{myBundle.ListProtestoChequeDetalleTitle_protestoChequeDetallePK_codigoMoneda}"/>
                    </f:facet>
                    <h:outputText value="#{item.protestoChequeDetallePK.codigoMoneda}"/>
                </p:column>
                <p:column sortBy="#{item.numeroCheques}" filterBy="#{item.numeroCheques}">
                    <f:facet name="header">
                        <h:outputText value="#{myBundle.ListProtestoChequeDetalleTitle_numeroCheques}"/>
                    </f:facet>
                    <h:outputText value="#{item.numeroCheques}"/>
                </p:column>
                <p:column sortBy="#{item.totalCheques}" filterBy="#{item.totalCheques}">
                    <f:facet name="header">
                        <h:outputText value="#{myBundle.ListProtestoChequeDetalleTitle_totalCheques}"/>
                    </f:facet>
                    <h:outputText value="#{item.totalCheques}"/>
                </p:column>
                <p:column sortBy="#{item.totalProtesto}" filterBy="#{item.totalProtesto}">
                    <f:facet name="header">
                        <h:outputText value="#{myBundle.ListProtestoChequeDetalleTitle_totalProtesto}"/>
                    </f:facet>
                    <h:outputText value="#{item.totalProtesto}"/>
                </p:column>
                <p:column sortBy="#{item.protestoCheque.codigo}" filterBy="#{item.protestoCheque.codigo}">
                    <f:facet name="header">
                        <h:outputText value="#{myBundle.ListProtestoChequeDetalleTitle_protestoCheque}"/>
                    </f:facet>
                    <h:outputText value="#{item.protestoCheque.codigo}"/>
                </p:column>
                <f:facet name="footer">
                    <div class="footer-section">
                        <p:commandButton id="createButton" icon="ui-icon-plus"   value="#{myBundle.Create}" actionListener="#{protestoChequeDetalleController.prepareCreate}" update=":ProtestoChequeDetalleCreateForm" oncomplete="ProtestoChequeDetalleCreateDialog.show()"/>
                        <p:commandButton id="viewButton"   icon="ui-icon-search" value="#{myBundle.View}" update=":ProtestoChequeDetalleViewForm" oncomplete="ProtestoChequeDetalleViewDialog.show()" disabled="#{empty protestoChequeDetalleController.selected}"/>
                        <p:commandButton id="editButton"   icon="ui-icon-pencil" value="#{myBundle.Edit}" update=":ProtestoChequeDetalleEditForm" oncomplete="ProtestoChequeDetalleEditDialog.show()" disabled="#{empty protestoChequeDetalleController.selected}"/>
                        <p:commandButton id="deleteButton" icon="ui-icon-trash"  value="#{myBundle.Delete}" actionListener="#{protestoChequeDetalleController.delete}" update=":growl,datalist" disabled="#{empty protestoChequeDetalleController.selected}"/>
                    </div>
                </f:facet>

            </p:dataTable>

        </p:panel>

    </h:form>

</ui:composition>