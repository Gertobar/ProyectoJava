package ec.renafipse.mks.clase.creditos;

/**
 *
 * @author Santiago Araujo
 */
public class PagoCreditoReestructuraCaptalCuotaVencida {
    
    private int reestructura;
    private int cuotasVencidas;
    private int cuotasCastigadas;
    public PagoCreditoReestructuraCaptalCuotaVencida( int reestructura, int cuotasVencidas, int cuotasCastigadas ){
        this.reestructura = reestructura;
        this.cuotasVencidas = cuotasVencidas;
        this.cuotasCastigadas = cuotasCastigadas;
    }

    /**
     * @return the reestructura
     */
    public int getReestructura() {
        return reestructura;
    }

    /**
     * @param reestructura the reestructura to set
     */
    public void setReestructura(int reestructura) {
        this.reestructura = reestructura;
    }

    /**
     * @return the cuotasVencidas
     */
    public int getCuotasVencidas() {
        return cuotasVencidas;
    }

    /**
     * @param cuotasVencidas the cuotasVencidas to set
     */
    public void setCuotasVencidas(int cuotasVencidas) {
        this.cuotasVencidas = cuotasVencidas;
    }
    
    /**
     * @return the cuotasCastigadas
     */
    public int getCuotasCastigadas() {
        return cuotasCastigadas;
    }

    /**
     * @param cuotasCastigadas the cuotasCastigadas to set
     */
    public void setCuotasCastigadas(int cuotasCastigadas) {
        this.cuotasCastigadas = cuotasCastigadas;
    }
}
