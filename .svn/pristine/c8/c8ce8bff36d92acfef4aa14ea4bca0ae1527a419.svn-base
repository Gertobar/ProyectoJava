<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:p="http://primefaces.org/ui">

    <h:form id="IfipAgenciaSecuenciaListForm">

        <p:panel header="#{propiedadesEtiquetaBL.ListIfipAgenciaSecuenciaTitle}">

            <p:dataTable id="datalist" value="#{ifipAgenciaSecuenciaController.items}" var="item"
                         selectionMode="single" selection="#{ifipAgenciaSecuenciaController.selected}"
                         rowKey="#{item.ifipAgenciaSecuenciaPK}"
                         paginator="true"
                         rows="10"
                         rowsPerPageTemplate="10,20,30"
                         >

                <p:ajax event="rowSelect"   update="createButton viewButton editButton deleteButton"/>
                <p:ajax event="rowUnselect" update="createButton viewButton editButton deleteButton"/>

                <p:column sortBy="#{item.valor}" filterBy="#{item.valor}">
                    <f:facet name="header">
                        <h:outputText value="#{propiedadesEtiquetaBL.ListIfipAgenciaSecuenciaTitle_valor}"/>
                    </f:facet>
                    <h:outputText value="#{item.valor}"/>
                </p:column>
                <p:column sortBy="#{item.secuencia.codigo}" filterBy="#{item.secuencia.codigo}">
                    <f:facet name="header">
                        <h:outputText value="#{propiedadesEtiquetaBL.ListIfipAgenciaSecuenciaTitle_secuencia}"/>
                    </f:facet>
                    <h:outputText value="#{item.secuencia.codigo}"/>
                </p:column>
                <p:column sortBy="#{item.ifipAgencia.codigo}" filterBy="#{item.ifipAgencia.codigo}">
                    <f:facet name="header">
                        <h:outputText value="#{propiedadesEtiquetaBL.ListIfipAgenciaSecuenciaTitle_ifipAgencia}"/>
                    </f:facet>
                    <h:outputText value="#{item.ifipAgencia.codigo}"/>
                </p:column>
                <f:facet name="footer">
                    <div class="footer-section">
                        <p:commandButton id="createButton" icon="ui-icon-plus"   value="#{propiedadesEtiquetaBL.Create}" actionListener="#{ifipAgenciaSecuenciaController.prepareCreate}" update=":IfipAgenciaSecuenciaCreateForm" oncomplete="IfipAgenciaSecuenciaCreateDialog.show()"/>
                        <p:commandButton id="viewButton"   icon="ui-icon-search" value="#{propiedadesEtiquetaBL.View}" update=":IfipAgenciaSecuenciaViewForm" oncomplete="IfipAgenciaSecuenciaViewDialog.show()" disabled="#{empty ifipAgenciaSecuenciaController.selected}"/>
                        <p:commandButton id="editButton"   icon="ui-icon-pencil" value="#{propiedadesEtiquetaBL.Edit}" update=":IfipAgenciaSecuenciaEditForm" oncomplete="IfipAgenciaSecuenciaEditDialog.show()" disabled="#{empty ifipAgenciaSecuenciaController.selected}"/>
                        <p:commandButton id="deleteButton" icon="ui-icon-trash"  value="#{propiedadesEtiquetaBL.Delete}" actionListener="#{ifipAgenciaSecuenciaController.delete}" update=":growl,datalist" disabled="#{empty ifipAgenciaSecuenciaController.selected}"/>
                    </div>
                </f:facet>

            </p:dataTable>

        </p:panel>

    </h:form>

</ui:composition>