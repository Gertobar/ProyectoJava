<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="UsuarioGuiaDepositoViewDlg" 
                  widgetVar="UsuarioGuiaDepositoViewDialog" 
                  modal="true" 
                  resizable="false" 
                  appendTo="@(body)" 
                  closeOnEscape="true">

            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.UsuarioGuiaDeposito}"
                               styleClass="encabezadosDialog"/>
            </f:facet>  


            <h:form id="UsuarioGuiaDepositoViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{usuarioGuiaDepositoController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.Usuario}"/>
                        <h:outputText value="#{usuarioGuiaDepositoController.selected.usuario.username}" 
                                      title="#{propiedadesTituloBL.Usuario}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Agencia}"/>
                        <h:outputText value="#{usuarioGuiaDepositoController.selected.ifipAgencia.nombre}" 
                                      title="#{propiedadesTituloBL.Agencia}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.HabilitadoPor}"/>
                        <h:outputText value="#{usuarioGuiaDepositoController.selected.habilitadoPor.username}"
                                      title="#{propiedadesTituloBL.HabilitadoPor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaHabilitacion}"/>
                        <h:outputText value="#{usuarioGuiaDepositoController.selected.fechaHabilitacion}" 
                                      title="#{propiedadesTituloBL.FechaHabilitacion}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <p:outputLabel value="#{propiedadesEtiquetaBL.Eliminado}" for="eliminado"/>
                        <p:selectOneMenu id="eliminado" 
                                         value="#{usuarioGuiaDepositoController.selected.eliminado}" 
                                         disabled="true"> 
                            <f:selectItem itemLabel="NO" itemValue="N" />  
                            <f:selectItem itemLabel="SI" itemValue="S" />                          
                        </p:selectOneMenu>
                    </p:panelGrid>
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
