<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="ParametroGeneralSegIfiViewDlg" 
                  widgetVar="ParametroGeneralSegIfiViewDialog" 
                  modal="true" 
                  resizable="false" 
                  appendTo="@(body)" 
                  closeOnEscape="true">

            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.ParametroGeneralSeguridad}"
                               styleClass="encabezadosDialog"/>
            </f:facet>

            <h:form id="ParametroGeneralSegIfiViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{parametroGeneralSegIfiController.selected != null}">                        


                        <p:outputLabel value="#{propiedadesEtiquetaBL.Ifip}" for="codigoIfip" />
                        <p:selectOneMenu id="codigoIfip" 
                                         value="#{parametroGeneralSegIfiController.selected.parametroGeneralSegIfiPK.codigoIfip}" 
                                         disabled="true">
                            <f:selectItem  itemValue="#{usuarioIfipAgenciaController.ifipAgencia.codigoIfip}"
                                           itemLabel="#{usuarioIfipAgenciaController.ifipAgencia.codigoIfip.nombre}"/>
                            <f:converter binding="#{ifipConverter}"/>
                        </p:selectOneMenu>

                        <h:outputText value="#{propiedadesEtiquetaBL.ValorNumerico}"/>
                        <h:outputText value="#{parametroGeneralSegIfiController.selected.valorNumerico}" 
                                      title="#{propiedadesTituloBL.ValorNumerico}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.ValorTexto}"/>
                        <h:outputText value="#{parametroGeneralSegIfiController.selected.valorTexto}"
                                      title="#{propiedadesTituloBL.ValorTexto}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.ValorFechaUno}"/>
                        <h:outputText value="#{parametroGeneralSegIfiController.selected.valorFechaUno}" 
                                      title="#{propiedadesTituloBL.ValorFechaUno}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.ValorFechaDos}"/>
                        <h:outputText value="#{parametroGeneralSegIfiController.selected.valorFechaDos}" 
                                      title="#{myBundle.ViewParametroGeneralSegIfiTitle_valorFechaDos}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Descripcion}"/>
                        <h:outputText value="#{parametroGeneralSegIfiController.selected.parametroGeneralSeguridad.descripcion}"
                                      title="#{propiedadesTituloBL.Descripcion}"/>

                        <p:outputLabel value="#{propiedadesEtiquetaBL.Eliminado}" for="eliminado"/>
                        <p:selectOneMenu id="eliminado" 
                                         value="#{parametroGeneralSegIfiController.selected.eliminado}" 
                                         disabled="true">    
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.No}" itemValue="N" />  
                            <f:selectItem itemLabel="#{propiedadesEtiquetaBL.Si}" itemValue="S" />                               
                        </p:selectOneMenu>

                    </p:panelGrid>
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
