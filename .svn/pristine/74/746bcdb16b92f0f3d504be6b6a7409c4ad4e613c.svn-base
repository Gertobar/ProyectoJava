            <p:ajax event="rowUnselect" update=":barraForm"/>

            <p:column filterBy="#{item.cedula}">
                <f:facet name="header">
                    <h:outputText value="#{propiedadesEtiquetaBL.Cedula}"/>
                </f:facet>
                <h:outputText value="#{item.cedula}"/>
            </p:column>

            <p:column>
                <f:facet name="header">
                    <h:outputText value="#{propiedadesEtiquetaBL.Ahorro}"/>
                </f:facet>
                <h:outputText value="#{item.ahorro}">
