<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="PeriodoEfeCheEntFinViewDlg" 
                  widgetVar="PeriodoEfeCheEntFinViewDialog" 
                  modal="true" resizable="false" 
                  appendToBody="true" 
                  closeOnEscape="true">
            

            <h:form id="PeriodoEfeCheEntFinViewForm">

               <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" columnClasses="column" rendered="#{periodoEfeCheEntFinController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.EntidadFinanciera}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.codigoEntidadFinanciera}" 
                                      title="#{propiedadesEtiquetaBL.EntidadFinanciera}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradoPor}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.registradoPor}"
                                      title="#{propiedadesEtiquetaBL.RegistradoPor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.fechaRegistro}"
                                      title="#{propiedadesEtiquetaBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.eliminado}"
                                      title="#{propiedadesEtiquetaBL.Eliminado}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.PeriodoEfeCheEntFin}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.codigoPeriodo.codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.EntidadFinanciera}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.entidadFinanciera.codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Ifip}"/>
                        <h:outputText value="#{periodoEfeCheEntFinController.selected.codigoIfip}"
                                      title="#{propiedadesEtiquetaBL.Ifip}"/>
                    </p:panelGrid>
                  
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
