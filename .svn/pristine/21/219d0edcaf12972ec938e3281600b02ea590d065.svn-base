<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="ConceptoTransaccionViewDlg" 
                  widgetVar="ProductoIfitViewDialog" 
                  modal="true" 
                  resizable="false" 
                  appendTo="@(body)" 
                  closeOnEscape="true">

            <f:facet name="header">
                <h:outputLabel value="#{propiedadesEtiquetaBL.Mostrar} #{propiedadesEtiquetaBL.Tasa}"
                               styleClass="encabezadosDialog"/>
            </f:facet>    

            <h:form id="ProductoIfitViewForm">

                <h:panelGroup id="display"  style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" rendered="#{tipoRubroProCreMonIfiController.selected != null}">


                        <h:outputText value="#{propiedadesEtiquetaBL.Codigo}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.codigo}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.TipoRubro}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.codigoTipoRubro.nombre}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.ProductoCredito}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.productoCreditoMonedaIfip.productoCreditoMoneda.productoCredito.nombre}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Moneda}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.productoCreditoMonedaIfip.productoCreditoMoneda.moneda.nombre}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.Ifip}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.productoCreditoMonedaIfip.ifip.nombre}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.Tipo}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.tipo}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.Valor} "/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.valor}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.CalculaSobre}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.calculaSobre}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.CobradoEn}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.cobradoEn}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.OrdenCobro}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.ordenCobro}"/>
                        
                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradaPor}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.usuarioRegistradoPor.username}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.fechaRegistro}"
                                      title="#{propiedadesTituloBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{tipoRubroProCreMonIfiController.selected.eliminado}"
                                      title="#{propiedadesTituloBL.Eliminado}"/>                       
                    </p:panelGrid>
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
