	<subDataset name="diasNotificacion" uuid="ceff97fa-e59d-48c8-bdf3-2b307e28e487">
		<parameter name="codIfip" class="java.lang.Long"/>
		<queryString>
			<![CDATA[select deni.dia_inicial||' a '||deni.dia_final DIAS,
       tn.nombre NOTIFICACION,
       tni.valor VALOR
from mks_creditos.tipo_notificacion tn,
     mks_creditos.tipo_notificacion_ifip tni,
     mks_creditos.dias_envio_notificacion_ifip deni,
     mks_ifips.ifip ifi
where tn.codigo=tni.codigo_tipo_notificacion
and   tni.codigo_tipo_notificacion=deni.codigo_tipo_notificacion
and   tni.codigo_ifip=ifi.codigo
and   deni.codigo_ifip=ifi.codigo
and   ifi.codigo=$P{codIfip}]]>
		</queryString>
		<field name="DIAS" class="java.lang.String"/>
		<field name="NOTIFICACION" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="numeroCredito" class="java.lang.Long"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/vicastoc/renafipse/unidad-tecnlogia/minkasoftware/desarrollo/minkasoftware/src/main/webapp/financiero/socios/socio/reporte/"]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoIfip" class="java.lang.Long"/>
	<parameter name="subReporteTablaAmortizacion" class="java.lang.Object"/>
	<parameter name="subReporteListadoGarantes" class="java.lang.Object"/>
	<parameter name="subReporteGarantes" class="java.lang.Object"/>
	<parameter name="subReporteAceptacionRubro" class="java.lang.Object"/>
	<queryString>
		<![CDATA[WITH
  conyuge AS (SELECT ppp.identificacion   identificacion,
                     ppp.nombre_completo  nombre,
                     pec.codigo_persona   codigo_persona
              FROM   mks_creditos.solicitud              sol,
                     mks_socios.socio                    soc,
                     mks_socios.persona_conyuge          pec,
                     mks_socios.persona                  ppp
              WHERE  sol.numero                          = $P{numeroCredito}
              AND    sol.codigo_ifip                     = $P{codigoIfip}
              AND    firma                               = 'S'
              AND    eliminado                           = 'N'
              AND    soc.codigo                          = sol.codigo_socio
              AND    soc.codigo_ifip                     = sol.codigo_ifip
              AND    pec.codigo_persona                  = soc.codigo_persona
              AND    ppp.codigo                          = pec.codigo_persona_conyuge)
SELECT soc.codigo                                                     "SOCIO No:",
       pers.nombre_completo                                            "NOMBRE:",
       pers.identificacion                                             "C.I.:",
       tii.nombre||' '||ifi.nombre                                    "IFIP",
       sol.numero                                                     "CREDITO No:",
       sol.monto_credito                                              "POR: $",
      -- sol.tasa                                                       "TASA",
       coy.nombre                                                     "NOMBRE CONYUGE",
       coy.identificacion                                             "IDENTIFICACION CONYUGE",
       (SELECT TO_CHAR(sysdate,'DD')FROM DUAL )                         "DIA",
       (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL)                     "MES",
       (SELECT TO_CHAR(sysdate,'YYYY') FROM DUAL)                       "ANIO",
       REPLACE( REPLACE(
                REPLACE(
                REPLACE(
                REPLACE(
                REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 15)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$SOCIO', pers.nombre_completo)
                ,'$COD_SOCIO', soc.codigo)
                ,'$LUGAR', ifa.nombre)
                ,'$CONYUGUE_SOCIO', pers.nombre_completo)
                ,'$IDENTIFICACION_CONTYUGUE_SOCIO', pers.identificacion)                            "INTRODUCCION",
REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 32)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "INTRODUCCION_II",
        REPLACE( REPLACE(
                        REPLACE((SELECT  rfi.texto
                        FROM    mks_ifips.reporte_fragmento_ifip rfi
                        where   codigo_reporte                     = 5
                        AND     codigo_fragmento                   = 5)
                        ,'$COAC',tii.nombre||' '||ifi.nombre)
                        ,'$FECHA_CREACION',TO_CHAR(soc.fecha_creacion,'DD/MM/YYYY'))
                        ,'$COD_SOCIO', soc.codigo)                                                 "PRIMERA_CLAUSULA",
        REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
              FROM    mks_ifips.reporte_fragmento_ifip rfi
              where   codigo_reporte                     = 5
              AND     codigo_fragmento                   = 6)
              ,'$COAC',tii.nombre||' '||ifi.nombre),
              '$MONTO_LETRAS',(mks_creditos.pkm_numero_a_letras.DinTex(NVL(sol.monto_credito,0)))||' ')
              ,'$ACTIVIDAD_ECONOMICA',(': '|| NVL((SELECT de.nombre FROM mks_creditos.destino_especifico de WHERE sol.codigo_destino_especifico = de.codigo)  ,sol.observaciones)) )    "SEGUNDA CLAUSULA",

               REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 7)
                ,'$MESES',ped.equivalencia_meses*sol.numero_cuotas)                                    "TERCERA_CLAUSULA",
         REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 31)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "TERCERA_CLAUSULA_II",
       REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 8)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                , '$TIR', TRIM(TO_CHAR(sol.tasa,'FM9999999990.00' )))
                , '$TEA', TRIM(TO_CHAR(mks_creditos.pkm_solicitud.f_obtiene_tea_contrato_con($P{numeroCredito}, $P{codigoIfip}),'FM9999999990.00' )))
                ,'$SEGMENTO', seg.nombre)                                                          "CUARTA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 16)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "QUINTA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 17)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "SEXTA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 18)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "SEPTIMA_CLAUSULA",
         REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 19)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "OCTAVA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 20)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "NOVENA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 21)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                              "DECIMA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 22)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                          "DECIMA_PRIMERA_CLAUSULA",
      (SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 23)                               "DECIMA_SEGUNDA_CLAUSULA",
      (SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 33)                               "DECIMA_SEGUNDA_CLAUSULA_II",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 24)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                          "DECIMA_TERCERA_CLAUSULA",
       REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 25)
                ,'$COAC',tii.nombre||' '||ifi.nombre)                                          "DECIMA_CUARTA_CLAUSULA",
       REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((SELECT  rfi.texto
                FROM    mks_ifips.reporte_fragmento_ifip rfi
                where   codigo_reporte                     = 5
                AND     codigo_fragmento                   = 26)
                ,'$COAC',tii.nombre||' '||ifi.nombre)
                ,'$LUGAR', ubc.nombre)
                ,'$DIA', (SELECT TO_CHAR(sysdate,'DD')FROM DUAL ))
                ,'$MES', (SELECT initcap(TRIM(TO_CHAR(SYSDATE, 'MONTH', 'NLS_DATE_LANGUAGE=SPANISH'))) FROM DUAL ))
                ,'$ANIO', (SELECT TO_CHAR(sysdate,'YYYY')FROM DUAL ))                                   "DECIMA_QUINTA_CLAUSULA"
FROM   mks_creditos.solicitud                                         sol,
       mks_socios.socio                                               soc,
       mks_socios.persona                                             pers,
       mks_ifips.ifip_agencia                                         ifa,
       mks_ifips.ifip                                                 ifi,
       mks_ifips.tipo_ifip                                            tii,
       mks_creditos.solicitud_pagare                                  sp,
       mks_comunes.ubicacion_geografica                               ubc,
       mks_socios.persona_actividad_economica                         pae,
       mks_socios.persona_act_eco_pri                                 paep,
       mks_socios.actividad_economica                                 aec,
       conyuge                                                        coy,
       mks_comunes.periodicidad                                       ped,
       --mks_creditos.solicitud_tir_tea                                 tasas,
       mks_ifips.segmento_ifip                                        seg
WHERE  sol.numero                                                     = $P{numeroCredito}
AND    ifi.codigo                                                     = $P{codigoIfip}
AND    sol.codigo_socio                                               = soc.codigo
AND    sol.codigo_ifip                                                = ifi.codigo
AND    sol.codigo_ifip_agencia                                        = ifa.codigo
AND    ifi.codigo_tipo_ifip                                           = tii.codigo
AND    soc.codigo_persona                                             = pers.codigo
AND    ubc.codigo                                                     = ifa.codigo_ubi_geo_ciudad
AND    sol.numero                                                     = sp.numero_credito
AND    pae.codigo_persona                                             = pers.codigo
AND    pae.codigo_actividad_economica                                 = paep.codigo_actividad_economica
AND    paep.codigo_actividad_economica                                = aec.codigo
AND    paep.codigo_persona                                            = pers.codigo
AND    soc.codigo_persona                                             = coy.codigo_persona (+)
AND    sol.codigo_periodicidad                                        = ped.codigo
--AND    tasas.numero_credito                                           = sol.numero
AND    seg.codigo                                                     = ifi.codigo_segmento_ifip]]>
	</queryString>
