<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TipoGarantiaViewDlg" widgetVar="TipoGarantiaViewDialog" modal="true" resizable="false"  appendToBody="true" header="#{myBundle.ViewTipoGarantiaTitle}" closeOnEscape="true">

            <h:form id="TipoGarantiaViewForm">

                <h:panelGroup id="display">
                    <p:panelGrid columns="2" columnClasses="column" rendered="#{tipoGarantiaController.selected != null}">

                        <h:outputText value="#{myBundle.ViewTipoGarantiaLabel_codigo}"/>
                        <h:outputText value="#{tipoGarantiaController.selected.codigo}" title="#{myBundle.ViewTipoGarantiaTitle_codigo}"/>

                        <h:outputText value="#{myBundle.ViewTipoGarantiaLabel_nombre}"/>
                        <h:outputText value="#{tipoGarantiaController.selected.nombre}" title="#{myBundle.ViewTipoGarantiaTitle_nombre}"/>

                        <h:outputText value="#{myBundle.ViewTipoGarantiaLabel_eliminado}"/>
                        <h:outputText value="#{tipoGarantiaController.selected.eliminado}" title="#{myBundle.ViewTipoGarantiaTitle_eliminado}"/>
                    </p:panelGrid>
                    <p:commandButton value="#{myBundle.Close}" onclick="TipoGarantiaViewDialog.hide()"/>
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
