<ui:component xmlns:ui="http://java.sun.com/jsf/facelets"
              xmlns:componentes ="http://java.sun.com/jsf/composite/ezcomp"
              xmlns:h="http://java.sun.com/jsf/html"
              xmlns:p="http://primefaces.org/ui">
    <h:form id="componentePersonaForm">
        <componentes:componenteBuscarPersona
            id="componenteBuscarPersona"
            widgetVar="listaPersonaDialog"
            onHide="cargarBeneficiario()"
