<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="IngresoEgresoMonedaViewDlg" 
                  widgetVar="IngresoEgresoMonedaViewDialog" 
                  modal="true" resizable="false" 
                  appendToBody="true" 
                  closeOnEscape="true">

            <h:form id="IngresoEgresoMonedaViewForm">

                <h:panelGroup id="display" style="font-size: 10px; margin-top: 0px;">
                    <p:panelGrid columns="2" columnClasses="column" rendered="#{ingresoEgresoMonedaController.selected != null}">

                        <h:outputText value="#{propiedadesEtiquetaBL.CodigoMoneda}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.ingresoEgresoMonedaPK.codigoMoneda}" 
                                      title="#{propiedadesEtiquetaBL.CodigoMoneda}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.Mostrar}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.mostrar}"
                                      title="#{propiedadesEtiquetaBL.Mostrar}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.RegistradoPor}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.registradoPor}"
                                      title="#{propiedadesEtiquetaBL.RegistradoPor}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.FechaRegistro}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.fechaRegistro}"
                                      title="#{propiedadesEtiquetaBL.FechaRegistro}">
                            <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                        </h:outputText>

                        <h:outputText value="#{propiedadesEtiquetaBL.Eliminado}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.eliminado}" 
                                      title="#{propiedadesEtiquetaBL.Eliminado}"/>

                        <h:outputText value="#{propiedadesEtiquetaBL.IngresoEgreso}"/>
                        <h:outputText value="#{ingresoEgresoMonedaController.selected.ingresoEgreso.descripcion}"/>
                    </p:panelGrid>
                   
                </h:panelGroup>

            </h:form>

        </p:dialog>

    </ui:composition>

</html>
